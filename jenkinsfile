pipeline {
    agent any
    tools {
        maven 'Maven_3.9.7'  // EXACT name from Jenkins Global Tool Configuration
    }
    stages {
        stage('SCM') {
            steps {
                git 'https://github.com/SagarlaVenuGopal/test-maven-venu.git'
            }
        }
        stage('version') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('install') {
            steps {
                sh 'mvn install'
            }
        }
    }
}
